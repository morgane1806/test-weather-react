{
  "name": "weather",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.3.0",
    "flow-bin": "^0.74.0",
    "immutable": "^3.8.2",
    "material-ui": "^0.20.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-redux-i18n": "^1.9.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.1.0",
    "redux": "^4.0.0",
